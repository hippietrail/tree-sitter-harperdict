{
  "$schema": "https://tree-sitter.github.io/tree-sitter/assets/schemas/grammar.schema.json",
  "name": "hdz",
  "rules": {
    "source_file": {
      "type": "SEQ",
      "members": [
        {
          "type": "SYMBOL",
          "name": "number"
        },
        {
          "type": "STRING",
          "value": "\n"
        },
        {
          "type": "REPEAT",
          "content": {
            "type": "SYMBOL",
            "name": "entry"
          }
        }
      ]
    },
    "entry": {
      "type": "SEQ",
      "members": [
        {
          "type": "SYMBOL",
          "name": "word"
        },
        {
          "type": "SEQ",
          "members": [
            {
              "type": "CHOICE",
              "members": [
                {
                  "type": "SYMBOL",
                  "name": "annotation"
                },
                {
                  "type": "BLANK"
                }
              ]
            },
            {
              "type": "CHOICE",
              "members": [
                {
                  "type": "SYMBOL",
                  "name": "whitespace_comment"
                },
                {
                  "type": "BLANK"
                }
              ]
            }
          ]
        },
        {
          "type": "STRING",
          "value": "\n"
        }
      ]
    },
    "annotation": {
      "type": "SEQ",
      "members": [
        {
          "type": "SYMBOL",
          "name": "slash"
        },
        {
          "type": "CHOICE",
          "members": [
            {
              "type": "SYMBOL",
              "name": "flags"
            },
            {
              "type": "BLANK"
            }
          ]
        }
      ]
    },
    "whitespace_comment": {
      "type": "SEQ",
      "members": [
        {
          "type": "SYMBOL",
          "name": "whitespace"
        },
        {
          "type": "CHOICE",
          "members": [
            {
              "type": "SYMBOL",
              "name": "comment"
            },
            {
              "type": "BLANK"
            }
          ]
        }
      ]
    },
    "number": {
      "type": "PATTERN",
      "value": "\\d+"
    },
    "word": {
      "type": "PATTERN",
      "value": "[0-9A-Za-z'áçéíïóü'.]+"
    },
    "slash": {
      "type": "STRING",
      "value": "/"
    },
    "flags": {
      "type": "PATTERN",
      "value": "[A-Za-z0-9'.!+~-]+"
    },
    "whitespace": {
      "type": "PATTERN",
      "value": "\\s+"
    },
    "comment": {
      "type": "PATTERN",
      "value": ".+"
    }
  },
  "extras": [
    {
      "type": "PATTERN",
      "value": "\\s"
    }
  ],
  "conflicts": [],
  "precedences": [],
  "externals": [],
  "inline": [],
  "supertypes": [],
  "reserved": {}
}